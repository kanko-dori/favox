{
  "name": "functions",
  "scripts": {
    "build": "npm run build:back && npm run build:front",
    "build:back": "tsc",
    "build:front": "next build src/frontend",
    "watch:back": "tsc --watch",
    "serve": "run-s build emulator",
    "emulator": "firebase emulators:start --only functions",
    "deploy": "firebase deploy",
    "logs": "firebase functions:log",
    "dev": "next src/frontend",
    "clean": "rimraf lib",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "test": "jest",
    "lint": "eslint src/** --fix",
    "lint:ci": "eslint src/**"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
    "express": "^4.17.1",
    "firebase": "^8.0.1",
    "firebase-admin": "^9.3.0",
    "firebase-functions": "^3.6.1",
    "next": "^10.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/webpack": "^4.41.24",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "firebase-functions-test": "^0.2.0",
    "jest": "^26.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.3",
    "typescript": "^3.8.0"
  },
  "private": true
}
